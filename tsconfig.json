{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "jsx": "react",
        "moduleResolution": "node",
        "strict": true,
        "noEmit": true,
        "allowJs": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "declaration": true,
    },
    "include": ["src"]
}